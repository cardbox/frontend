{
  "presets": ["patronum/babel-preset"],
  "plugins": [
    ["effector/babel-plugin", { "addLoc": true }],
    [
      "effector/babel-plugin",
      {
        "noDefaults": true,
        "importName": "lib/postpone",
        "attachCreators": ["postpone"]
      },
      "lib/postpone"
    ],
    [
      "styled-components",
      {
        "displayName": true,
        "ssr": true
      }
    ]
  ]
}
